<%= render "layouts/container-start" %>
    <h1>Saved Photos</h1>
    <p id="notice"><%= notice %></p>
	<div class="row photo-grid">
	  <% unless @saved_photos.blank? %>	
	      <% @saved_photos.each do |photo| %>
	      	<a href="<%= edit_photo_path(id: photo.id) %>">
	          <div class="photo-wrapper col-xs-4 col-sm-4 col-md-4">
	            <%= image_tag(photo.url_url(:standard), class: "img-responsive") %>
	          </div> 
	        </a>
	      <% end %>
	  <% end %>
	</div> <!-- Row -->
<%= render "layouts/container-start" %>